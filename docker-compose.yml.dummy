version: '3.7'
#v5

default:
  script: sh "${TEST_VAR}"
services:
  django_gunicorn:
    volumes:
      - static:/static
    env_file:
      - .env
    build:
      context: .
    ports:
      - "8000:8000"
  nginx:
    build: ./nginx
    volumes:
      - static:/static
    ports:
      - "234:80"
    depends_on:
      - django_gunicorn

volumes:
  static:


